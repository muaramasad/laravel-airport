{
  "name": "muaramasad/laravel-airport-package",
  "description": "Laravel airport package",
  "license": "MIT",
  "authors": [
    {
      "name": "Muaramasad",
      "email": "hellovnv@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Muaramasad\\Airport\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Muaramasad\\Airport\\Tests\\": "src/Tests"
    }
  },
  "require": {
    "php" : ">=7.2.0",
    "illuminate/support": "^6.0|^7.0",
    "doctrine/dbal": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "squizlabs/php_codesniffer": "^3.0",
    "mockery/mockery": "^1.0",
    "orchestra/testbench": "^4.0"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    },
    "laravel": {
      "providers": [
        "Muaramasad\\Airport\\ServiceProvider"
      ]
    }
  }
}
